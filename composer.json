{
    "name": "filippo-toso/models-generator",
    "description": "A Laravel Artisan command to automatically generate models from database tables",
    "license": "MIT",
    "authors": [{
        "name": "Filippo Toso",
        "email": "filippo.toso@creativepark.it"
    }],
    "require": {
        "php": "^7.0 || ^8.0 || ^9.0",
        "illuminate/support": ">=8.0",
        "laravel/helpers": "^1.3",
        "doctrine/dbal": "^2.8 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "FilippoToso\\ModelsGenerator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilippoToso\\ModelsGenerator\\ServiceProvider"
            ]
        }
    }
}